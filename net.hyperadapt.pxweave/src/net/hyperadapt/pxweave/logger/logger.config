#
log4j.logger.net.hyperadapt.pxweave=DEBUG, weaverexecution_appender_file, weaverexecution_appender_console

log4j.appender.weaverexecution_appender_console=org.apache.log4j.ConsoleAppender
log4j.appender.weaverexecution_appender_file=org.apache.log4j.FileAppender
log4j.appender.weaverexecution_appender_file.File=xmlweaver_execution.log
log4j.appender.weaverexecution_appender_file.Append=false

log4j.appender.weaverexecution_appender_console.layout=org.apache.log4j.PatternLayout
log4j.appender.weaverexecution_appender_console.layout.ConversionPattern=%d{ABSOLUTE} %5p - %m (PX-Weave:%c{1}:%L)%n
log4j.appender.weaverexecution_appender_file.layout=org.apache.log4j.PatternLayout
log4j.appender.weaverexecution_appender_file.layout.ConversionPattern=%d{ABSOLUTE} %5p - %m (PX-Weave:%c{1}:%L)%n